					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[7]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[8]"/>
							<property alias="/params/rotor/rpm1[7]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[9]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain8"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain9"/>
						<property alias="/params/rotor/gain8"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[8]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[9]"/>
							<property alias="/params/rotor/rpm1[8]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>

	<input alias="/params/rotor/gain9"/>

	<output alias="/params/rotor/_gain"/>

</PropertyList>