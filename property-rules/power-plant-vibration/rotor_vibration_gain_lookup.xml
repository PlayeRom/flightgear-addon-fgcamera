<?xml version="1.0"?>

<PropertyList>

	<type>gain</type>

	<enable>
		<condition>
			<property alias="/params/rotor/enabled"/>
			<property>/sim/fgcamera/current-camera/effects/power-plant-vibration/enabled</property>
		</condition>
	</enable>

	<gain>0.001</gain>

	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[0]"/>
			</less-than-equals>
		</condition>
		<property alias="/params/rotor/gain0"/>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[1]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain0"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain1"/>
						<property alias="/params/rotor/gain0"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[0]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[1]"/>
							<property alias="/params/rotor/rpm1[0]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[2]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain1"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain2"/>
						<property alias="/params/rotor/gain1"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[1]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[2]"/>
							<property alias="/params/rotor/rpm1[1]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[3]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain2"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain3"/>
						<property alias="/params/rotor/gain2"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[2]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[3]"/>
							<property alias="/params/rotor/rpm1[2]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[4]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain3"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain4"/>
						<property alias="/params/rotor/gain3"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[3]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[4]"/>
							<property alias="/params/rotor/rpm1[3]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[5]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain4"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain5"/>
						<property alias="/params/rotor/gain4"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[4]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[5]"/>
							<property alias="/params/rotor/rpm1[4]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[6]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain5"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain6"/>
						<property alias="/params/rotor/gain5"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[5]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[6]"/>
							<property alias="/params/rotor/rpm1[5]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[7]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain6"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain7"/>
						<property alias="/params/rotor/gain6"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm1[6]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm1[7]"/>
							<property alias="/params/rotor/rpm1[6]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm1[8]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/gain7"/>
				<product>
					<difference>
						<property alias="/params/rotor/gain8"/>
