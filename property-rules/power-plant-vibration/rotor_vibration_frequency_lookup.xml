<?xml version="1.0"?>

<PropertyList>

	<type>gain</type>

	<enable>
		<condition>
			<property alias="/params/rotor/enabled"/>
			<property>/sim/fgcamera/current-camera/effects/power-plant-vibration/enabled</property>
		</condition>
	</enable>

	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm[0]"/>
			</less-than-equals>
		</condition>
		<property alias="/params/rotor/f0"/>
	</input>

	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm[1]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/f0"/>
				<product>
					<difference>
						<property alias="/params/rotor/f1"/>
						<property alias="/params/rotor/f0"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm[0]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm[1]"/>
							<property alias="/params/rotor/rpm[0]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>

	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm[2]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/f1"/>
				<product>
					<difference>
						<property alias="/params/rotor/f2"/>
						<property alias="/params/rotor/f1"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm[1]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm[2]"/>
							<property alias="/params/rotor/rpm[1]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>

	<input>
		<condition>
			<less-than-equals>
				<property>/rotors/main/rpm</property>
				<property alias="/params/rotor/rpm[3]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/rotor/f2"/>
				<product>
					<difference>
						<property alias="/params/rotor/f3"/>
						<property alias="/params/rotor/f2"/>
					</difference>
					<difference>
						<property>/rotors/main/rpm</property>
						<property alias="/params/rotor/rpm[2]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/rotor/rpm[3]"/>
							<property alias="/params/rotor/rpm[2]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
