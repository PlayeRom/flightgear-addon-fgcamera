	<input>
		<condition>
			<property>/sim/fgcamera/dialogs/RND/edit-mode</property>
		</condition>
		<value>1</value>
	</input>

	<input>
		<condition>
			<less-than-equals>
				<property alias="/params/speed"/>
				<property alias="/params/v1[0]"/>
			</less-than-equals>
		</condition>
		<property alias="/params/gain[0]"/>
	</input>
	<input>
		<condition>
			<less-than-equals>
				<property alias="/params/speed"/>
				<property alias="/params/v1[1]"/>
			</less-than-equals>
		</condition>
		<expression>
			<sum>
				<property alias="/params/gain[0]"/>
				<product>
					<difference>
						<property alias="/params/gain[1]"/>
						<property alias="/params/gain[0]"/>
					</difference>
					<difference>
						<property alias="/params/speed"/>
						<property alias="/params/v1[0]"/>
					</difference>
					<pow>
						<difference>
							<property alias="/params/v1[1]"/>
							<property alias="/params/v1[0]"/>
						</difference>
						<value>-1</value>
					</pow>
				</product>
			</sum>
		</expression>
	</input>
