<?xml version="1.0"?>

<PropertyList>

	<type>gain</type>

	<!--enable>
		<condition>
			<property alias="/params/rotor/enabled"/>
			<property>/sim/fgcamera/current-camera/effects/power-plant-vibration/enabled</property>
		</condition>
