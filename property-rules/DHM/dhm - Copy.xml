		<input>
			<expression>
				<difference>
					<property alias="/params/dhm/u1-y"/>
					<product>
						<property>/DHM/landing_output</property>
						<property>/DHM/landing-y-level</property>
						<property>/DHM/landing_output_scale</property>
					</product>
				</difference>
			</expression>
		</input>
		<!--output alias="/params/dhm/y"/-->
	</filter>


	<filter>
		<type>gain</type>
		<gain>
			<condition>
				<less-than>
					<property alias="/params/dhm/u1-y"/>
					<value>0</value>
				</less-than>
			</condition>
			<value>-1</value>
		</gain>
		<gain>1.0</gain>
		<input>
			<expression>
				<product>
					<property alias="/params/dhm/scale"/>
					<difference>
						<property alias="/params/dhm/lim-y"/>
						<product>
							<property alias="/params/dhm/lim-y"/>
							<pow>
								<value>0.5</value>
								<abs>
									<property alias="/params/dhm/u1-y"/>
								</abs>
							</pow>
						</product>
					</difference>
				</product>
			</expression>
		</input>
		<output alias="/params/dhm/y"/>
	</filter>

	<filter>
		<type>gain</type>
		<gain>
			<condition>
				<less-than>
					<property alias="/params/dhm/u1-z"/>
					<value>0</value>
				</less-than>
			</condition>
			<value>-1</value>
		</gain>
		<gain>1.0</gain>
		<input>
			<expression>
				<product>
					<property alias="/params/dhm/scale"/>
					<difference>
						<property alias="/params/dhm/lim-z"/>
						<product>
							<property alias="/params/dhm/lim-z"/>
							<pow>
								<value>0.5</value>
								<abs>
									<property alias="/params/dhm/u1-z"/>
								</abs>
							</pow>
						</product>
					</difference>
				</product>
			</expression>
		</input>
		<output alias="/params/dhm/z"/>
	</filter>

	<filter>
		<type>gain</type>
		<gain>
			<condition>
				<less-than>
					<property alias="/params/dhm/u1-x"/>
					<value>0</value>
				</less-than>
			</condition>
			<value>-1</value>
		</gain>
		<gain>1.0</gain>
		<input>
			<expression>
				<product>
					<property alias="/params/dhm/scale"/>
					<difference>
						<property alias="/params/dhm/lim-x"/>
