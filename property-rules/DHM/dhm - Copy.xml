	<filter>
		<type>gain</type>
		<enable>0</enable>
		<gain>
			<property alias="/params/dhm/scale"/>
		</gain>
		<!--input>
			<condition>
				<greater-than-equals>
					<property alias="/params/dhm/u1-y"/>
					<property alias="/params/dhm/lim-y"/>
				</greater-than-equals>
			</condition>
			<expression>
				<sum>
					<property alias="/params/dhm/lim-y"/>
