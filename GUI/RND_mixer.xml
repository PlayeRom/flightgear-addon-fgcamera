		<button>
			<legend>Generator 1...</legend>
			<pref-width>140</pref-width>
			<pref-height>30</pref-height>
			<binding>
				<command>property-assign</command>
				<property>/sim/fgcamera/dialogs/RND/generator/current-generator</property>
				<value>0</value>
			</binding>
			<binding>
				<command>dialog-show</command>
				<dialog-name>RND-generator</dialog-name>
			</binding>
			<binding>
				<command>dialog-close</command>
				<dialog-name>RND-mixer</dialog-name>
			</binding>
		</button>

		<button>
			<legend>Generator 2...</legend>
			<pref-width>140</pref-width>
			<pref-height>30</pref-height>
			<binding>
				<command>property-assign</command>
				<property>/sim/fgcamera/dialogs/RND/generator/current-generator</property>
				<value>1</value>
			</binding>
			<binding>
				<command>dialog-show</command>
				<dialog-name>RND-generator</dialog-name>
			</binding>
			<binding>
				<command>dialog-close</command>
				<dialog-name>RND-mixer</dialog-name>
			</binding>
		</button>

		<button>
			<legend>Generator 3...</legend>
			<pref-width>140</pref-width>
			<pref-height>30</pref-height>
