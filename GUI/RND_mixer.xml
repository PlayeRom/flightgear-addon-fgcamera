				<dialog-name>RND-mixer</dialog-name>
			</binding>
		</button>

		<empty>
			<stretch>true</stretch>
		</empty>

		<text>
			<label>Edit</label>
		</text>

		<checkbox>
			<width>15</width>
			<height>15</height>
			<property>/sim/fgcamera/dialogs/RND/edit-mode</property>
			<binding>
				<command>dialog-apply</command>
			</binding>
			<binding>
				<command>nasal</command>
				<script>toggle_edit()</script>
			</binding>
		</checkbox>

		<button>
			<legend>Ground</legend>
			<pref-width>80</pref-width>
			<pref-height>30</pref-height>
			<enable>
				<property>/sim/fgcamera/dialogs/RND/edit-mode</property>
				<property>/sim/fgcamera/dialogs/RND/profile</property>
			</enable>
			<binding>
				<command>nasal</command>
				<script>fgcamera.RND_handler.GUI_mode = 0; update()</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/sim/fgcamera/dialogs/RND/profile</property>
				<value>0</value>
			</binding>
		</button>

		<button>
			<legend>Air</legend>
			<pref-width>50</pref-width>
			<pref-height>30</pref-height>
			<enable>
				<property>/sim/fgcamera/dialogs/RND/edit-mode</property>
				<not>
					<property>/sim/fgcamera/dialogs/RND/profile</property>
				</not>
			</enable>
			<binding>
				<command>nasal</command>
				<script>fgcamera.RND_handler.GUI_mode = 1; update()</script>
			</binding>
			<binding>
				<command>property-assign</command>
				<property>/sim/fgcamera/dialogs/RND/profile</property>
				<value>1</value>
			</binding>
		</button>
	</group>

