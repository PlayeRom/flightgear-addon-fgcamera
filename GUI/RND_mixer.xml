					<label>1.00</label>
					<format>%0.2f</format>
					<live>true</live>
					<property>/sim/fgcamera/dialogs/RND/mixer/r/GEN3</property>
				</text>
			</group>

			<group>
				<layout>hbox</layout>

				<dial>
					<width>20</width>
					<wrap>0</wrap>
					<property>/sim/fgcamera/dialogs/RND/mixer/r/GEN1</property>
					<binding>
						<command>dialog-apply</command>
					</binding>
					<binding>
						<command>nasal</command>
						<script>set_data("r", 0, 10)</script>
					</binding>
				</dial>

				<dial>
					<width>20</width>
					<wrap>0</wrap>
					<property>/sim/fgcamera/dialogs/RND/mixer/r/GEN2</property>
					<binding>
						<command>dialog-apply</command>
					</binding>
					<binding>
						<command>nasal</command>
						<script>set_data("r", 1, 10)</script>
					</binding>
				</dial>

				<dial>
					<width>20</width>
					<wrap>0</wrap>
					<property>/sim/fgcamera/dialogs/RND/mixer/r/GEN3</property>
					<binding>
						<command>dialog-apply</command>
					</binding>
					<binding>
						<command>nasal</command>
						<script>set_data("r", 2, 10)</script>
					</binding>
				</dial>
			</group>
			<group>
				<layout>hbox</layout>
				<text>
					<label>GEN 1</label>
				</text>

				<text>
					<label>GEN 2</label>
				</text>

				<text>
					<label>GEN 3</label>
				</text>
			</group>
		</group>
		<vrule/>
		<group>
			<layout>vbox</layout>
				<empty> <stretch>1</stretch> </empty>

				<text>
					<label>OUTPUT</label>
				</text>

				<dial>
					<width>20</width>
					<wrap>0</wrap>
					<property>/sim/fgcamera/dialogs/RND/mixer/s</property>
					<min>0</min>
					<max>1</max>
					<binding>
						<command>dialog-apply</command>
					</binding>
					<binding>
						<command>nasal</command>
						<script>set_scale()</script>
					</binding>
				</dial>

				<text>
					<pref-width>50</pref-width>
					<label>-1.00</label>
					<format>%-0.2f</format>
					<live>true</live>
					<property>/sim/fgcamera/dialogs/RND/mixer/s</property>
				</text>

				<button>
					<legend>Curves...</legend>
					<pref-width>90</pref-width>
					<pref-height>30</pref-height>
					<binding>
						<command>dialog-show</command>
						<dialog-name>RND-curves</dialog-name>
					</binding>
					<binding>
						<command>dialog-close</command>
						<dialog-name>RND-mixer</dialog-name>
					</binding>
				</button>

			<empty> <stretch>1</stretch> </empty>

			</group>
	</group>

	<hrule/>

	<group>
	<layout>hbox</layout>
		<text>
			<label>Current effect:</label>
		</text>

		<button>
			<legend>Copy</legend>
			<pref-width>110</pref-width>
			<pref-height>30</pref-height>
			<binding>
				<command>nasal</command>
				<script>copy_effect()</script>
			</binding>
		</button>

		<button>
			<legend>Paste</legend>
			<pref-width>110</pref-width>
			<pref-height>30</pref-height>
			<enable>
				<not><property>sim/fgcamera/dialogs/RND/mixer/clipboard-empty</property></not>
			</enable>
			<binding>
				<command>nasal</command>
				<script>paste_effect()</script>
			</binding>
		</button>

		<button>
			<legend>Import...</legend>
			<pref-width>110</pref-width>
			<pref-height>30</pref-height>
			<binding>
				<command>dialog-show</command>
				<dialog-name>RND-import</dialog-name>
			</binding>
		</button>

		<button>
			<legend>Export</legend>
			<pref-width>110</pref-width>
			<pref-height>30</pref-height>
			<enable>
				<property></property>
			</enable>
			<!--binding>
				<command>nasal</command>
				<script>copy_effect()</script>
			</binding-->
		</button>
	</group>

	<nasal>
		<open> <![CDATA[
			var path  = "/sim/fgcamera/dialogs/RND/mixer/";
			var list1 = ["/GEN1", "/GEN2", "/GEN3", "/level"];
			var list2 = ["x", "y", "z", "h", "p", "r"];
			var mode  = nil;
			var listener1 = setlistener( "/sim/fgcamera/current-camera/RND-updated", func {update()} );

			var update = func {
				mode  = get_mode();

				foreach (var dof; list2) {
					var i = 0;
					foreach (var g; list1) {
						setprop ( path ~ dof ~ g, fgcamera.rnd[mode].mixer[dof][i] );
						i += 1;
					}
				}

				setprop("/sim/fgcamera/dialogs/RND/mixer/s", fgcamera.rnd[mode].mixer.s);

				fgcommand( "dialog-update", props.Node.new( {"dialog-name" : "RND-mixer"} ) );
			}

			var get_mode = func  {
				var GUI_edit = fgcamera.RND_handler.GUI_edit;
				var GUI_mode = fgcamera.RND_handler.GUI_mode;

				if ( GUI_edit ) GUI_mode;
				else fgcamera.RND_handler._mode;
			}

			var toggle_edit = func fgcamera.RND_handler.GUI_edit = getprop("/sim/fgcamera/dialogs/RND/edit-mode");

			var set_scale = func fgcamera.rnd[mode].mixer.s = getprop("/sim/fgcamera/dialogs/RND/mixer/s");

			var set_data = func (key, i, scale = 1) fgcamera.rnd[mode].mixer[key][i] = getprop ( path ~ key ~ list1[i] );


			fgcamera.rnd_effectN = props.Node.new();
			setprop(path ~ "clipboard-empty", 1);

			var copy_effect = func {
				fgcamera.rnd_effectN.removeAllChildren();
				fgcamera.rnd_effectN.setValues(fgcamera.rnd[mode]);
				setprop(path ~ "clipboard-empty", 0);
			}

			var paste_effect = func {
				fgcamera.rnd[mode] = fgcamera.rnd_effectN.getValues();
				fgcamera.update_rnd_data();
				fgcamera.RND_handler._set_generators();
				update();
			}




			update();
		]]> </open>
		<close>
			removelistener(listener1);
		</close>
	</nasal>
</PropertyList>
