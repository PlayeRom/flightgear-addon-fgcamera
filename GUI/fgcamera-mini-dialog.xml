<?xml version="1.0"?>

<PropertyList>
    <name>fgcamera-mini-dialog</name>
    <layout>hbox</layout>
    <color>
        <alpha>0.0</alpha>
    </color>
    <x>10</x>
    <y>10</y>
    <draggable>false</draggable>
    <resizable>false</resizable>

    <button>
        <legend><![CDATA[<<]]></legend>
        <pref-width>30</pref-width>
        <pref-height>30</pref-height>
        <binding>
            <command>fgcamera-prev-category</command>
        </binding>
    </button>

    <button>
        <legend><![CDATA[<]]></legend>
        <pref-width>30</pref-width>
        <pref-height>30</pref-height>
        <binding>
            <command>fgcamera-prev-in-category</command>
        </binding>
    </button>

    <button>
        <legend>R</legend>
        <pref-width>30</pref-width>
        <pref-height>30</pref-height>
        <binding>
            <command>fgcamera-select</command>
            <camera-id>0</camera-id>
        </binding>
    </button>

    <button>
        <legend><![CDATA[>]]></legend>
        <pref-width>30</pref-width>
        <pref-height>30</pref-height>
        <binding>
            <command>fgcamera-next-in-category</command>
        </binding>
    </button>

    <button>
        <legend><![CDATA[>>]]></legend>
        <pref-width>30</pref-width>
        <pref-height>30</pref-height>
        <binding>
            <command>fgcamera-next-category</command>
        </binding>
    </button>

</PropertyList>
